  "scripts": {
    "code-review": "npm-run-all --parallel lint-html find-clones test:onerun",
    "start": "npm-run-all --parallel test lint:watch",
    "test": "echo angularjs unit tests merged into angular unit tests",
    "test:onerun": "echo angularjs unit tests merged into angular unit tests",
    "test:onerunlocal": "echo angularjs unit tests merged into angular unit tests",
    "test:watch": "echo angularjs unit tests merged into angular unit tests",
    "lint": "node_modules/.bin/esw app",
    "lintlocal": "node_modules/.bin/esw app && node ts-pending-lint.js -overrideTfLocation",
    "lint:watch": "npm run lint -- --watch",
    "lint-html": "eslint app/**/*.js --format html --output-file ./reports/lint-results.html",
    "lint-fix": "eslint --fix app/**/*.js",
    "find-clones-build": "jsinspect -I -L --threshold 50 --ignore \"tests|common.js\" ./app --reporter default || exit 0",
    "find-clones": "jsinspect -I -L --threshold 50 --ignore \"tests|common.js\" ./app",
    "lesshint": "lesshint \"Content\" \"ng-app/src\" --max-warnings",
    "ng": "ng",
    "serve:local": "start \"NG SERVE LOCAL\" \"%CD%\\nodeScripts\\serve.cmd\" local",
    "serve": "start \"NG SERVE\" \"%CD%\\nodeScripts\\serve.cmd\"",
    "serve:kiosk": "start \"NG SERVE KIOSK\" \"%CD%\\nodeScripts\\serve.cmd\" kiosk",
    "ng-build": "pushd ng-app && ng build && popd",
    "ng-test": "pushd nodeScripts && node testPreBuild.js && popd && pushd ng-app && ng test && popd",
    "ng-test:onerun": "pushd nodeScripts && node testPreBuild.js && SET failfocus=1 && popd && pushd ng-app && ng test --watch=false && SET failfocus=0 && popd",
    "ng-test:codecoverage": "pushd nodeScripts && node testPreBuild.js && popd && pushd ng-app && ng test --watch=false --code-coverage && popd",
    "ng-lint": "pushd ng-app && ng lint && popd",
    "ng-ngjs-test-build": "echo angularjs unit tests merged into angular unit tests",
    "build:stats": "pushd nodeScripts && node prodPreBuild.js && popd && pushd ng-app && ng run ng-app:build:production --outputPath=../Client_build/Web --stats-json && popd",
    "analyze": "pushd ng-app && webpack-bundle-analyzer ../Client_build/stats.json",
    "localBuild": "npm run build:all",
    "build:all": "npm run build:web",
    "build:angularjs": "echo angularjs build handled in build:web now",
    "build:web": "pushd ng-app && ng run ng-app:build:local --outputPath=../Client_build/Web && popd",
    "tfsBuild": "npm run tfsBuild:allseq",
    "tfsBuild:allseq": "npm run tfsBuild:web",
    "tfsBuild:all": "npm run tfsBuild:web",
    "tfsBuild:web": "pushd nodeScripts && node prodPreBuild.js && popd && pushd ng-app && ng run ng-app:build:production --outputPath=../Client_build/Web && popd"
  },
